{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:svelte/recommended",
		"plugin:svelte/prettier",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"extraFileExtensions": [".svelte"]
	},
	"rules": { "svelte/no-dupe-style-properties": "warn" }
}
