{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"prettier",
		"plugin:svelte/recommended",
		"plugin:svelte/prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": { "svelte/no-dupe-style-properties": "warn" }
}
